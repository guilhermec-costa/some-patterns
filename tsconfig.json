{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "*": ["*"]
    }
  },
  "include": ["src/**/*", "index.ts", "creational"],
  "exclude": ["node_modules", "dist"]
}
